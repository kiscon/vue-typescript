{
  "name": "vue3-ts",
  "version": "0.1.1",
  "author": "kiscon",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest=server/",
    "lint": "vue-cli-service lint",
    "test-build": "vue-cli-service build --mode=test --dest=server/",
    "dev-build": "vue-cli-service build --mode=development --dest=server/"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.0.0-rc.9",
    "@vue/cli-plugin-babel": "^3.0.0-rc.4",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.4",
    "@vue/cli-service": "^3.0.0-rc.4",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "typescript": "^2.9.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
