{
  "name": "vue3-ts",
  "version": "0.1.1",
  "author": "kiscon",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test-build": "vue-cli-service build --mode=test --dest=server/",
    "dev-build": "vue-cli-service build --mode=development --dest=server/",
    "start": "live-server ./dist",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.8.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-service": "^3.8.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "live-server": "^1.2.1",
    "style-resources-loader": "^1.2.1",
    "typescript": "^3.5.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
